<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid_info">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".MaskActivity"></activity>
        <activity android:name=".VaccineActivity" />
        <activity
            android:name=".Popup"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SituationActivity" />
        <activity android:name=".NoticeActivity" />
        <activity android:name=".MapActivity" />
        <receiver android:name=".receiveAlarm" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyD4jMo5xAnK9qPytKfcEWedDCuhkWEOLFY"/>
    </application>

</manifest>